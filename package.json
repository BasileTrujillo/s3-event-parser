{
  "name": "@digitregroup/s3-event-parser",
  "version": "1.0.0",
  "description": "Parse AWS S3/SNS events",
  "author": "DigitRE group",
  "keywords": [
    "digitregroup",
    "s3",
    "aws"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix .",
    "lint:ci": "eslint . --format junit --output-file ./reports/eslint.xml",
    "test": "nyc mocha ./test/*.js",
    "test:ci": "nyc mocha ./test/*.js --reporter=html --reporter=mocha-junit-reporter --reporter-options mochaFile=./reports/junit.xml",
    "nsp": "nsp check"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/digitregroup/s3-event-parser.git"
  },
  "devDependencies": {
    "@digitregroup/eslint-config": "^1.7.0",
    "chai": "^4.1.2",
    "eslint": "^4.16.0",
    "mocha": "^5.0.5",
    "mocha-junit-reporter": "^1.17.0",
    "mock-require": "^3.0.2",
    "nsp": "^3.2.1",
    "nyc": "^11.6.0"
  },
  "dependencies": {
    "joi": "^13.2.0"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "src/*.js"
    ],
    "ignore-class-method": "methodToIgnore",
    "reporter": [
      "html",
      "text"
    ],
    "reporter-options": {
      "mochaFile": "./reports/junit.xml"
    },
    "extension": [
      ".js"
    ],
    "report-dir": "./reports"
  }
}
